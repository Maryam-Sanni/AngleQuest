{
  "expo": {
    "name": "recruitangle",
    "slug": "recruitangle",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/Recruitangle Icon.png",
      "sourceDir": "."
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": {
            "Varta-Light": "./assets/fonts/Varta-Light.ttf",
            "Varta-Bold":"./assets/fonts/Varta-Bold.ttf",
            "Varta-Medium":"./assets/fonts/Varta-Medium.ttf"
          }
        }
      ]
    ]
  }
}
